    "        known_rates = zerocurve.loc[:years-0.25]\n",
    "        zerorate = bootstrap(known_rates.values,known_rates.index,years,\n",
    "                             swaprate,years,0.25)\n",
    "        zerocurve.loc[years] = zerorate\n"
   ]
